
############
# General records
# 

record(seq, "$(P)READ") {
	field(DESC, "Initiator for all items needing read")
	field(SCAN, "1 second")
	
	field(DO1, "1")
	field(DO2, "1")
	field(DO3, "1")
	field(DO4, "1")
	field(DO5, "1")
	field(DO6, "1")
	field(DO7, "1")
	field(DO8, "1")
	field(DO9, "1")
	field(DOA, "1")

	field(LNK1, "$(P)MODE.PROC")
	field(LNK2, "$(P)1:SENSOR.PROC")
	field(LNK3, "$(P)2:SENSOR.PROC")
	field(LNK4, "$(P)1:TEMP:SP:RBV.PROC")
	field(LNK5, "$(P)2:TEMP:SP:RBV.PROC")
	field(LNK6, "$(P)1:TEMP:SENSOR.PROC")
	field(LNK7, "$(P)2:TEMP:SENSOR.PROC")
	field(LNK8, "$(P)1:HEAT.PROC")
	field(LNK9, "$(P)1:P.PROC")
	field(LNKA, "$(P)2:P.PROC")
	
	field(FLNK, "$(P)ACTIVITY")
	
	field(SELM, "All")
}

record(calc,"$(P)ACTIVITY") {
   field(DESC, "Activity record")
   field(INPA, "$(P)ACTIVITY")
   field(CALC, "A!=0?0:1")
   field(VAL, 0)
   field(PINI, "YES")
}

############
# Units Mode
# 

record(mbbi, "$(P)MODE")
{
	field(DESC, "Mode")
    field(SCAN, "Passive")
    field(DTYP, "stream")
    field(INP,  "@main.proto getMode() $(PORT)")
	
	field(ZRVL, 0)
	field(ZRST, "Monitor")
	field(ONVL, 1)
	field(ONST, "Control")
	field(TWVL, 2)
	field(TWST, "Auto Tune")
	field(THVL, 3)
	field(THST, "Off")
	
	info(INTEREST, "MEDIUM")
	info(archive, "VAL")
}

record(mbbo, "$(P)MODE:SP")
{
	field(DESC, "Set mode")
    field(DTYP, "stream")
    field(OUT,  "@main.proto setMode() $(PORT)")
	
	field(ZRVL, 0)
	field(ZRST, "Monitor")
	field(ONVL, 1)
	field(ONST, "Control")
	
	info(archive, "VAL")
}

alias("$(P)MODE:SP", "$(P)MODE:SP:RBV")

############
# Temperature Sensor 1
# 

record(ai, "$(P)1:SENSOR")
{
	field(DESC, "Temperature of sensor 1")
	field(SCAN, "Passive")
    field(DTYP, "stream")
    field(INP,  "@main.proto getTemp(1,$(P)1:SENSOR:UNITS) $(PORT)")
	field(PREC, 3)

	info(archive, "VAL")
}
record(stringin, "$(P)1:SENSOR:UNITS")
{
	field(DESC, "Temperature sensor 2 units")
	field(SCAN, "Passive")
}

