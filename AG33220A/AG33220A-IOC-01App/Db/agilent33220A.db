record(bo, "$(P)SIM")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(ZNAM, "NO")
    field(ONAM, "YES")
}

record(bo, "$(P)DISABLE") 
{
    field(DESC, "Disable comms")
    field(PINI, "YES")
    field(VAL, "0")
    field(OMSL, "supervisory")
    field(ZNAM, "COMMS ENABLED")
    field(ONAM, "COMMS DISABLED")
}

record(mbbi, "$(P)UNITS")
{
    field(DTYP, "stream")
    field(INP, "@agilent33220A.proto getUnits $(PORT)")
    field(SCAN, "1 second")

    field(ZRVL, 0)
    field(ZRST, "VPP")
    field(ONVL, 1)
    field(ONST, "VRMS")
    field(TWVL, 2)
    field(TWST, "DBM") 
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:UNITS")
    field(SDIS, "$(P)DISABLE")
}

record(mbbo, "$(P)UNITS:SP")
{
    field(DTYP, "stream")
    field(OUT, "@agilent33220A.proto setUnits $(PORT)")

    field(ZRVL, 0)
    field(ZRST, "VPP")
    field(ONVL, 1)
    field(ONST, "VRMS")
    field(TWVL, 2)
    field(TWST, "DBM") 
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:UNITS:SP")
    field(SDIS, "$(P)DISABLE")
}

alias("$(P)UNITS", "$(P)UNITS:SP:RBV")

