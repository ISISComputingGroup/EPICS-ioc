#######################################################
#
# DB file for Keithley 2400 Source Meter
#
#######################################################

record(bo, "$(P)SIM")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(ZNAM, "NO")
    field(ONAM, "YES")
    field(VAL, "$(RECSIM=0)")
}

record(bo, "$(P)DISABLE")
{
    field(DESC, "Disable comms")
    field(PINI, "YES")
    field(VAL, "$(DISABLE=0)")
    field(OMSL, "supervisory")
    field(ZNAM, "COMMS ENABLED")
    field(ONAM, "COMMS DISABLED")
}

#####################
#### Output mode ####
#####################

record(bi, "$(P)OUTPUT:MODE") {
    field(DESC, "Output mode Off/On")
    field(DTYP, "stream")
    field(SCAN, "Passive")
    field(INP, "@Keithley2400.proto get_OUTPUT_MODE $(PORT)")
    field(ZNAM, "Off")
    field(ONAM, "On")
    field(SDIS, "$(P)DISABLE")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:OUTPUT:MODE")
    field(FLNK, "$(P)_READ_TRIGGER")
    info(archive, "VAL")
    info(INTEREST, "HIGH")
}

record(bo, "$(P)OUTPUT:MODE:SP") {
    field(DESC, "Set output mode Off/On")
    field(DTYP, "stream")
    field(OUT, "@Keithley2400.proto set_OUTPUT_MODE $(PORT)")
    field(ZNAM, "Off")
    field(ONAM, "On")
    field(VAL, "1")
    field(SDIS, "$(P)DISABLE")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:OUTPUT:MODE:SP")
    info(archive, "VAL")
    info(INTEREST, "HIGH")
}

record(bi, "$(P)SIM:OUTPUT:MODE")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(ZNAM, "Off")
    field(ONAM, "On")
}

alias("$(P)OUTPUT:MODE", "$(P)OUTPUT:MODE:SP:RBV")
alias("$(P)SIM:OUTPUT:MODE", "$(P)SIM:OUTPUT:MODE:SP")
alias("$(P)SIM:OUTPUT:MODE", "$(P)SIM:OUTPUT:MODE:SP:RBV")

#######################################
#### Process reads if output is on ####
#######################################


record(fanout, "$(P)_READ_OUTPUT") {
    field(SCAN, "1 second")
    field(LNK1, "$(P)OUTPUT:MODE PP")
    field(SELM, "All")
}

record(seq, "$(P)_READ_MODES") {

    field(SCAN, "2 second")

    field(DO1, "1")
    field(DO2, "1")
    field(DO3, "1")
    field(DO4, "1")
    field(DO5, "1")
    field(DO6, "1")
    field(DO7, "1")
    field(DO8, "1")

    field(DLY1, "0.1")
    field(DLY2, "0.1")
    field(DLY3, "0.1")
    field(DLY4, "0.1")
    field(DLY5, "0.1")
    field(DLY6, "0.1")
    field(DLY7, "0.1")
    field(DLY8, "0.1")

    field(LNK1, "$(P)SENS:MODE.PROC")
    field(LNK2, "$(P)OCOM.PROC")
    field(LNK3, "$(P)RES:MODE.PROC")
    field(LNK4, "$(P)RES:RANGE:AUTO.PROC")
    field(LNK5, "$(P)RES:RANGE.PROC")
    field(LNK6, "$(P)SOURCE:MODE.PROC")
    field(LNK7, "$(P)I:COMPLIANCE.PROC")
    field(LNK8, "$(P)_READ_SOURCES.PROC")

    field(SELM, "All")
}

record(seq, "$(P)_READ_SOURCES") {

    field(SCAN, "2 second")

    field(DO1, "1")
    field(DO2, "1")
    field(DO3, "1")
    field(DO4, "1")
    field(DO5, "1")
    field(DO6, "1")
    field(DO7, "1")
    field(DO8, "1")

    field(DLY1, "0.1")
    field(DLY2, "0.1")
    field(DLY3, "0.1")
    field(DLY4, "0.1")
    field(DLY5, "0.1")
    field(DLY6, "0.1")
    field(DLY7, "0.1")
    field(DLY8, "0.1")


    field(LNK1, "$(P)V:COMPLIANCE.PROC")
    field(LNK2, "$(P)VOLT:SOURCE:SP:RBV.PROC")
    field(LNK3, "$(P)CURR:SOURCE:SP:RBV.PROC")
    field(LNK4, "$(P)CURR:SOURCE:AUTORANGE:SP:RBV.PROC")
    field(LNK5, "$(P)VOLT:SOURCE:AUTORANGE:SP:RBV.PROC")
    field(LNK6, "$(P)_READ_MODES.PROC")



    field(SELM, "All")
}

record(calcout, "$(P)_READ_TRIGGER") {
  field(INPA, "$(P)OUTPUT:MODE.RVAL")
  field(CALC, "A==1?0:1")
  field(OOPT, "When Zero")
  field(OUT, "$(P)_READ_POWER.PROC")
}

record(seq, "$(P)_READ_POWER") {

    field(SCAN, "Passive")
    field(DO1, "1")
    field(DO2, "1")
    field(DO3, "1")

    field(DLY1, "0")
    field(DLY2, "0")
    field(DLY3, "0")

    field(LNK1, "$(P)V:RAW.PROC")
    field(LNK2, "$(P)I:RAW.PROC")
    field(LNK3, "$(P)R:RAW.PROC")

    field(SELM, "All")
}

######################################
#### Current, Voltage, Resistance ####
######################################

record(ai, "$(P)I:RAW")
{
    field(DTYP, "stream")
    field(INP, "@Keithley2400.proto get_I $(PORT)")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:I")
    field(SDIS, "$(P)DISABLE")
}

record(ai, "$(P)SIM:I")
{
}

record(calcout, "$(P)I:CALC")
{
    field(INPA, "$(P)I:RAW CP MS")
    # The reason this is done is to filter out the 10**38 "Nan"
    # value that the device gives when sensor disconnected.
    field(CALC, "A>10**20?Nan:A")
}

record(ai, "$(P)I")
{
    field(DESC, "Current")
    field(EGU, "A")
    field(PREC, "5")
    field(INP, "$(P)I:CALC CP MS")
    info(archive, "VAL")
    info(INTEREST, "HIGH")
}

