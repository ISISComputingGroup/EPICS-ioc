pattern {P,      N,     M,         DTYP,      PORT,  ADDR,    DESC,        EGU,     DIR,  VELO,  VBAS,  ACCL,  BDST,  BVEL,  BACC,  MRES,  PREC,  DHLM,  DLLM,  INIT}
{"\$(P)", 1, "\$(M)", "asynMotor",  "\$(PORT)",    "\$(ADDR)",  "\$(M) (PIROT)",  mm,  Pos,  1,     0,    .2,    0,     .5,     .2,    0.001953125,  6,     125,   -125,  ""}
}
