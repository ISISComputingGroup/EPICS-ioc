file riken_psus.template {
#
# PSNAME = Name of the IOC. e.g. "RB1", "RB2"
# PSIOC  = IOC which this power supply is on. E.g. RKNPS_01
# PSIDX  = Letter A-L. Must be unique
#
pattern {PV_PREFIX, P, PSU_DISABLE, PSNAME, PSIOC, PSIDX}

    {"\$(PV_PREFIX)", "\$(P)", "\$(PSU_DISABLE)", "RQ18", "RKNPS_01", "A"}
    {"\$(PV_PREFIX)", "\$(P)", "\$(PSU_DISABLE)", "RQ19", "RKNPS_01", "B"}
    {"\$(PV_PREFIX)", "\$(P)", "\$(PSU_DISABLE)", "RQ20", "RKNPS_01", "C"}
}
