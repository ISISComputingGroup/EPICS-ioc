record(bi, "$(P)RB3:BANNER") {
    field(INP, "$(P)RB3:POWER CP MS")
    field(ZNAM, "off; ports 1,2 safe")
    field(ONAM, "on; beam to ports 1,2")
    field(OSV, "MAJOR")
}

record(bi, "$(P)RB4:BANNER") {
    field(INP, "$(P)RB4:POWER CP MS")
    field(ZNAM, "off; ports 3,4 safe")
    field(ONAM, "on; beam to ports 3,4")
    field(OSV, "MAJOR")
}
