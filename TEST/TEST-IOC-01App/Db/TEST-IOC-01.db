
record(ai,"$(P)VAL")
{
    field(DTYP,"$(RANDDEV)")
    field(DESC,"Random numbers")
    field(SCAN,"1 second")
    field(INP,"$(RANDSEED)")
    field(LINR,"LINEAR")
    field(ESLO,1)
    field(EOFF,0)
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM::VAL")
    field(SDIS, "$(P)DISABLE")
}

record(ai, "$(P)VALUE1")
{
    field(VAL, 1)
    field(HIHI,18)
    field(HIGH,15)
    field(LOW,5)
    field(LOLO,2)
    field(HHSV,"MAJOR")
    field(HSV,"MINOR")
    field(LSV,"MINOR")
    field(LLSV,"MAJOR")
    field(SIML, "$(P):SIM")
    field(SIOL, "$(P):SIM:VALUE1")
    field(SDIS, "$(P):DISABLE")
}

record(ai, "$(P)VALUE2")
{
    field(VAL, 2)
    field(SIML, "$(P):SIM")
    field(SIOL, "$(P):SIM:VALUE2")
    field(SDIS, "$(P):DISABLE")
}

record(calc,"$(P)DIFF")
{
    field(SCAN,"1 second")
    field(INPA, "$(P):VALUE1")
    field(INPB, "$(P):VALUE2")
    field("CALC", "A - B")
}
### SIMULATION RECORDS ###

record(bo, "$(P)SIM")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(ZNAM, "NO")
    field(ONAM, "YES")
    info("testparam","testvalue")
}

record(bo, "$(P)DISABLE") 
{
    field(DESC, "Disable comms")
    field(PINI, "YES")
    field(VAL, "0")
    field(OMSL, "supervisory")
    field(ZNAM, "COMMS ENABLED")
    field(ONAM, "COMMS DISABLED")
}

record(ai, "$(P)SIM:VALUE1")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
}

record(ai, "$(P)SIM:VALUE2")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
}

record(ai, "$(P)SIM:VAL")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
}

