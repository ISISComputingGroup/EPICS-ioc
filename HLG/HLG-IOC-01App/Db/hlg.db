#
# Main database file
#
# Macros:
#    P - prefix for ioc PVs
#    PORT - name of the asyn stream port to use
#    RECSIM - rec sim mode
#    DISABLE - disable communications

record(ai, "$(P)LEVEL")
{
	field(DESC, "Helium level on probe A")
    field(EGU, "mm")
    field(SCAN, "1 second")
    field(DTYP, "stream")
    field(INP, "@devhlg.proto getLevel $(PORT)")
    field(PREC, 2)
    info(INTEREST, "HIGH")
    info(archive, "VAL")
    info(alarm, "HLG")
}
