record (ai, "$(P)CHOPPER:PIT:TEMP") {

    field(DESC, "Chopper Pit Temperature")
    $(IFNOTRECSIM) field(INP,  "@asyn($(PORT)pit_temperature,0)FLOAT32_LE")
    $(IFNOTRECSIM) field(SCAN, "I/O Intr")
    $(IFNOTRECSIM) field(DTYP, "asynFloat64")

    field(EGU,  "C")
    field(PREC, "3")

    info(INTEREST, "HIGH")
    # info(ARCHIVE,  "10 VAL")

    field(SDIS, "$(P)DISABLE")
    field(SSCN, "Passive")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:CHOPPER:PIT:TEMP CP")
}

