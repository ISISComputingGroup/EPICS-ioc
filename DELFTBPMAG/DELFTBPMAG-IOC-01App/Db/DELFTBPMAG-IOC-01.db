record(mbbi, "$(P)STATUS")
{
    field(DTYP, "stream")
    field(DESC, "Status")
    field(INP,  "@DELFTBPMAG.proto readStatus $(PORT)")
	field(ZRVL, 0)
	field(ZRST, "Control")
	field(ONVL, 1)
	field(ONST, "Stop")
	field(TWVL, 2)
	field(TWST, "Hold")
    field(SCAN, "1 second")
    info(INTEREST, "MEDIUM")
    info(archive, "VAL")
}

