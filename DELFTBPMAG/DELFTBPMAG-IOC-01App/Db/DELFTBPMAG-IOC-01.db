record(bo, "$(P)CONTROL:START")
{
    field(DTYP, "stream")
    field(DESC, "Start Controlling")
    field(OUT,  "@DELFTBPMAG.proto startControlling $(PORT)")
    info(INTEREST, "MEDIUM")
    info(archive, "VAL")
}

