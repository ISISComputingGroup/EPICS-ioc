record(bi, "$(P)POWER")
{
    field(SCAN, "1 second")
    field(DESC, "Power")
    field(DTYP, "stream")
    field(INP, "@TDK_Lambda_Genesys.proto power $(PORT)")
    field(ZNAM, "Off")
    field(ONAM, "On")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:POWER:SP")
    field(SDIS, "$(P)DISABLE")
