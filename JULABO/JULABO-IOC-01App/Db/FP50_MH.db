record(ao, "$(P)TEMP:SP") 
{
    field(SCAN, "Passive")
    field(DTYP, "stream")
    field(OUT,  "@julaboCommon.proto writeTemp $(PORT)")
    field(EGU, "C")
    field(PREC, "1")
	field(SIOL, "$(P)SIM:TEMP:SP")
	field(SIML, "$(P)SIM")
    field(SDIS, "$(P)DISABLE")
    info(INTEREST, "HIGH")
    field(DESC, "Temperature Setpoint")
}

