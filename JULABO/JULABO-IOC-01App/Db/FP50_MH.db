# Julabo only has one setpoint. To enable doing csets on external temperature,
# alias the setpoint PVs to also apply to exttemp PVs
alias("$(P)TEMP:SP", "$(P)EXTTEMP:SP")
alias("$(P)TEMP:SP:RBV", "$(P)EXTTEMP:SP:RBV")

