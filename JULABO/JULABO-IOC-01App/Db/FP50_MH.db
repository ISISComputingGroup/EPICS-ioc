### SIMULATION RECORDS ###

record(stringout, "$(P)SIM:VERSION")
{
	field(DTYP, "Soft Channel")
	field(VAL, "Simulated Julabo FP50_MH")
}

record(stringout, "$(P)SIM:STATUS")
{
	field(DTYP, "Soft Channel")
	field(VAL, "Simulated")
}


record(ao, "$(P)SIM:TEMP:SP")
{
	field(DTYP, "Soft Channel")
}

record(calc, "$(P)SIM:TEMP")
{
	field(DTYP, "Soft Channel")
	field(INPA, "$(P)SIM:TEMP:SP PP")
	field(CALC, "A + 0.2")
}

record(bo, "$(P)SIM:MODE")
{
	field(DTYP, "Soft Channel")
	field(ZNAM, "OFF")
    field(ONAM, "ON")
}

record(ao, "$(P)SIM:POWER")
{
	field(DTYP, "Soft Channel")
	field(VAL, "75")
}

record(calc, "$(P)SIM:EXTTEMP")
{
	field(DTYP, "Soft Channel")
	field(INPA, "$(P)SIM:TEMP PP")
	field(CALC, "25 + A")
}

record(ao, "$(P)SIM:HIGHLIMIT")
{
	field(DTYP, "Soft Channel")
	field(VAL, "1000")
}

record(ao, "$(P)SIM:LOWLIMIT")
{
	field(DTYP, "Soft Channel")
	field(VAL, "0")
}

alias ("$(P)SIM:TEMP:SP","$(P)SIM:TEMP:SP:RBV")
alias ("$(P)SIM:MODE","$(P)SIM:MODE:SP")