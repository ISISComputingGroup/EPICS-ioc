<?xml version="1.0" ?>
<ioc_config xmlns:xi="http://www.w3.org/2001/XInclude">
<config_part>
<ioc_desc>Huber Motor Controller</ioc_desc>
<ioc_details></ioc_details>
<macros>
<xi:include href="../../../COMMON/PORT.xml" />
<macro name="MTRCTRL" pattern="^[0-9]{1,2}$" description="Controller number used in motor address assignment" hasDefault="NO" />
<macro name="NUMBERAXES" pattern="^([1-9]|(1[1-6]))$" description="Number of Axes on the Controller" defaultValue="5" hasDefault="YES" />
<macro name="BAUD" pattern="^[0-9]+$" description="Baud rate (default: 19200)" defaultValue="19200" hasDefault="YES" />
<macro name="PARITY" pattern="^(even|odd|none)$" description="Parity (default: none)" defaultValue="none" hasDefault="YES" />
<macro name="STOP" pattern="^([1-2])$" description="STOP bits" defaultValue="1" hasDefault="YES" />
<macro name="BITS" pattern="^([5-8])$" description="Data bits" defaultValue="8" hasDefault="YES" />
</macros>
<pvsets>
</pvsets>
</config_part>
</ioc_config>
