#### Reset ####
###############

record(ao, "$(P)RESET")
{
    field(DESC, "Reset the device")
    field(DTYP, "stream")
    field(OUT, "@Keithley2400.proto set_RESET $(PORT)")
    field(SDIS, "$(P)DISABLE")
}

