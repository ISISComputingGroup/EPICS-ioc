#######################################################
#
# DB file for Keithley 2400 Source Meter
#
#######################################################

record(bo, "$(P)SIM")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(ZNAM, "NO")
    field(ONAM, "YES")
    field(VAL, "$(RECSIM=0)")
}

record(bo, "$(P)DISABLE")
{
    field(DESC, "Disable comms")
    field(PINI, "YES")
    field(VAL, "$(DISABLE=0)")
    field(OMSL, "supervisory")
    field(ZNAM, "COMMS ENABLED")
    field(ONAM, "COMMS DISABLED")
}

######################################
#### Current, Voltage, Resistance ####
######################################

#Fanout reads so that they can be interrupted

record(fanout, "$(P)READ") {
	field(SCAN, ".5 second")
	field(LNK1, "$(P)V PP")
	field(LNK2, "$(P)I PP")
	field(LNK3, "$(P)R PP")
	field(SELM, "All")
}

record(ai, "$(P)I")
{
    field(DESC, "Current")
    field(DTYP, "stream")
    field(EGU, "A")
	field(PREC, "2")
    field(INP, "@Keithley2400.proto get_I $(PORT)")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:I")
    field(SDIS, "$(P)DISABLE")
    info(archive, "VAL")
    info(INTEREST, "HIGH")
}

record(ai, "$(P)SIM:I")
{
}

record(ai, "$(P)V")
{
    field(DESC, "Voltage")
    field(DTYP, "stream")
    field(EGU, "V")
	field(PREC, "2")
    field(INP, "@Keithley2400.proto get_V $(PORT)")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:V")
    field(SDIS, "$(P)DISABLE")
    info(archive, "VAL")
    info(INTEREST, "HIGH")
}

record(ai, "$(P)SIM:V")
{
}

record(ai, "$(P)R")
{
    field(DESC, "Resistance")
    field(DTYP, "stream")
    field(EGU, "Ohm")
	field(PREC, "2")
    field(INP, "@Keithley2400.proto get_R $(PORT)")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:R")
    field(SDIS, "$(P)DISABLE")
