record(bo, "$(P)SIM")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(ZNAM, "NO")
    field(ONAM, "YES")
    field(VAL, "$(RECSIM=0)")
	field(PINI, "YES")
}
    
record(bo, "$(P)DISABLE") 
{
    field(DESC, "Disable comms")
    field(PINI, "YES")
    field(VAL, "$(DISABLE=0)")
    field(OMSL, "supervisory")
    field(ZNAM, "COMMS ENABLED")
    field(ONAM, "COMMS DISABLED")
}

record(ao, "$(P)CHANNEL")
{
	field(DESC, "Current control channel")
    field(DTYP, "stream")
    field(EGU, "s")
    field(INP,  "@devInstron.proto getCurrentControlChannel ")
    field(SCAN, ".1 second")
    field(SIML, "$(P)SIM")
    field(SDIS, "$(P)DISABLE")
}

# Write to LabVIEW control "Reading Interval (seconds)" on "C:\LabVIEW Modules\Instruments\Enginx\Stress Rig\Stress Rig - System Functions.llb\Stress Rig - 100 kN Stress Rig.vi"
record(ai, "$(P)CHANNEL:SP")
{
	field(DESC, "Control channel setpoint")
    field(DTYP, "stream")
    field(EGU, "s")
    field(OUT,  "@devInstron.proto setCurrentControlChannel 2")
    field(SIML, "$(P)SIM")
    field(SDIS, "$(P)DISABLE")
}
