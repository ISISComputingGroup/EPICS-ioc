record(calc, "$(P)POS:TO")
{
    field(INPA,  "$(P)POS:RAW")
    # field(INPB,  "$(P)POS:CHAN:SCALE")    
    field(CALC, "A*1000*1")
    field(OUT, "$(P)POS PP")
}

record(calc, "$(P)POS:FROM")
{
    field(INPA,  "$(P)POS:SP")
    # field(INPB,  "$(P)POS:CHAN:SCALE")  
    field(CALC, "A*1000*1")
    field(OUT, "$(P)POS:RAW:SP PP")
}

