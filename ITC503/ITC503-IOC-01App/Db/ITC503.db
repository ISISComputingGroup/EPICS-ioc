record(bo, "$(P)SIM")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(ZNAM, "NO")
    field(ONAM, "YES")
    field(VAL, "$(RECSIM=0)")
    field(PINI, "YES")
}

record(bo, "$(P)DISABLE")
{
    field(DESC, "Disable comms")
    field(PINI, "YES")
    field(VAL, "$(DISABLE=0)")
    field(OMSL, "supervisory")
    field(ZNAM, "COMMS ENABLED")
    field(ONAM, "COMMS DISABLED")
}

record(ao, "$(P)P:SP") {
  field(DESC, "Set PID P")
  field(SCAN, "Passive")
  field(DTYP, "stream")
  field(OUT, "@itc503.proto set_p $(PORT)")
  field(OMSL, "supervisory")
  field(PREC, "2")
  field(DRVH, "999.99")
  field(DRVL, "0")
  field(SIML, "$(P)SIM")
  field(SIOL, "$(P)SIM:P:SP")
  field(SDIS, "$(P)DISABLE")
}

