record(waveform, "$(P)ED:USERNAME:DAE:SP")
{
    field(DESC, "Set Experiment User names")
	field(DTYP, "Soft Channel")
	field(SCAN, "Passive")
    field(NELM, "1024")
    field(FTVL, "CHAR")
	field(OUTP, "$(P)DAE:_USERNAME:SP")
