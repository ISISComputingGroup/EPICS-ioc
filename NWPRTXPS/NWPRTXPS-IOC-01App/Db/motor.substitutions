{P,     R,          PORT,       CHAN,   SCAN}
{IOC:,  XPSAux1Li,  XPS_AUX1,   0,      "I/O Intr"}
{IOC:,  XPSAux2Li,  XPS_AUX1,   1,      "I/O Intr"}
{IOC:,  XPSAux3Li,  XPS_AUX1,   2,      "I/O Intr"}
{IOC:,  XPSAux4Li,  XPS_AUX1,   3,      "I/O Intr"}
}

