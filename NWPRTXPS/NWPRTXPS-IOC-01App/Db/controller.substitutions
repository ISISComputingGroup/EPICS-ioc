file "$(MOTOR)/motorApp/Db/XPSAuxLo.db"
{
pattern
{P,     R,         PORT,     CHAN}
{IOC:,  XPSAux1Lo, "\$(AUX_PORT)", 0}
{IOC:,  XPSAux3Lo, "\$(AUX_PORT)", 1}
{IOC:,  XPSAux4Lo, "\$(AUX_PORT)", 2}
}


file "$(MOTOR)/motorApp/Db/XPSAuxAi.db"
{
pattern
{P,    R,          PORT,     CHAN, LOPR,   HOPR,  PREC, SCAN}
{IOC:, XPSAuxAi0,  "\$(AUX_PORT)", 0,    -10.0,  10.0,  4,    "1 second"}
{IOC:, XPSAuxAi1,  "\$(AUX_PORT)", 1,    -10.0,  10.0,  4,    "1 second"}
{IOC:, XPSAuxAi2,  "\$(AUX_PORT)", 2,    -10.0,  10.0,  4,    "1 second"}
{IOC:, XPSAuxAi3,  "\$(AUX_PORT)", 3,    -10.0,  10.0,  4,    "1 second"}
}

file "$(MOTOR)/motorApp/Db/XPSAuxAo.db"
{
pattern
{P,    R,          PORT,     CHAN, DRVL,   LOPR,   DRVH,  HOPR,  PREC}
{IOC:, XPSAuxAo0,  "\$(AUX_PORT)", 0,    -10.0,  -10.0,  10.0,  10.0,   4}
{IOC:, XPSAuxAo1,  "\$(AUX_PORT)", 1,    -10.0,  -10.0,  10.0,  10.0,   4}
{IOC:, XPSAuxAo2,  "\$(AUX_PORT)", 2,    -10.0,  -10.0,  10.0,  10.0,   4}
{IOC:, XPSAuxAo3,  "\$(AUX_PORT)", 3,    -10.0,  -10.0,  10.0,  10.0    4}
}
