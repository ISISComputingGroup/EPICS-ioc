file "$(MOTOR)/motorApp/Db/XPSAuxLi.db"
{
pattern
{P,     R,          PORT,       CHAN,   SCAN}
{IOC:,  XPSAux1Li,  "\$(AUX_PORT)",   0,      "I/O Intr"}
{IOC:,  XPSAux2Li,  "\$(AUX_PORT)",   1,      "I/O Intr"}
{IOC:,  XPSAux3Li,  "\$(AUX_PORT)",   2,      "I/O Intr"}
{IOC:,  XPSAux4Li,  "\$(AUX_PORT)",   3,      "I/O Intr"}
}

