  field(SCAN, "Passive")
  field(ZRST, "At rest")
  field(ZRVL, "0")
  field(ONST, "Sweeping")
  field(ONVL, "1")
  field(TWST, "Sweep Limiting")
  field(TWVL, "2")
  field(THST, "Swping & Lmting")
  field(THVL, "3")
  
  field(SIML, "$(P)SIM")
  field(SIOL, "$(P)SIM:STS:SWEEPMODE:SWEEP")
  field(SDIS, "$(P)DISABLE")
  
