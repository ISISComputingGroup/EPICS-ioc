  field(PINI, "YES")
  field(FLNK, "$(P)MODEL")
  field(SCAN, "Passive")
  field(SIML, "$(P)SIM")
  field(SIOL, "$(P)SIM:DONOTHING")
  field(SDIS, "$(P)DISABLE")
}

