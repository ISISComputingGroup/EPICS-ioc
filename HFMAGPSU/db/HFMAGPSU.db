record(mbbo, "$(P)DIRECTION:SP")
{
	field(DESC, "Set Direction - 0, +ve, -ve")
	field(DTYP, "stream")
	field(SDIS, "$(P):DISABLE")
	field(ZRST, "Zero")
	field(ZRVL, "0")              # method values correspond to 0/1/2 values? or literal?
	field(ONST, "-")
	field(ONVL, "1")
	field(TWST, "+")
	field(TWVL, "2")
	field(OUT, "@devHFMAGPSU.proto setDirection $(PORT)")
	field(SIML, "$(P):SIM")
	field(SIOL, "$(P):SIM:DIRECTION:SP PP")
	info(INTEREST, "HIGH")
	info(archive, "VAL")
 }
 
