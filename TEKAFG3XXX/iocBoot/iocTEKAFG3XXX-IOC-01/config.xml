<?xml version="1.0" ?>
<ioc_config xmlns:xi="http://www.w3.org/2001/XInclude">
<config_part>
<ioc_desc>Tektronix AFG3XXX Function Generator</ioc_desc>
<macros>
<macro name="ADDR" pattern="^[0-9]+\.[0-9]+\.[0-9]+\.[0-9]+$" description="IP address of the device" hasDefault="NO" />
<macro name="CHANNEL1" pattern="^(yes)|(no)$" description="Channel 1 is enabled" hasDefault="YES" defaultValue="yes" />
<macro name="CHANNEL2" pattern="^(yes)|(no)$" description="Channel 2 is enabled" hasDefault="YES" defaultValue="yes" />
</macros>
</config_part>
</ioc_config>
