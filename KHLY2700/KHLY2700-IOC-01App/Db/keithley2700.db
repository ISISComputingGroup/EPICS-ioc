    field(OUT,  "@devKeithley_2700.proto setBufferSize $(PORT)")
    field(HIHI, "55001")
    field(LOLO, "1")
    field(HHSV,"MAJOR")
    field(LLSV,"MAJOR")
	info(ARCHIVE, "VAL")
    info(INTEREST, "HIGH")
}
alias("$(P)BUFFER:SIZE:SP", "$(P)BUFFER:SIZE")

