#Fanout reads so that they can be interrupted

record(seq, "$(P)READ") {

	field(SCAN, "Passive")
	field(DO1, "1")
	field(DO2, "1")
	field(DO3, "1")
	field(DO4, "1")
	field(DO5, "1")
	field(DO6, "1")
	field(DO7, "1")
	field(DO8, "1")
	
	field(DLY1, "$(TREAD)")
	field(DLY2, "$(TREAD)")
	field(DLY3, "$(TREAD)")
	field(DLY4, "$(TREAD)")
	field(DLY5, "$(TREAD)")
	field(DLY6, "$(TREAD)")
	field(DLY7, "$(TREAD)")
	field(DLY8, "$(TREAD)")
	
	field(LNK1, "$(P)READ_FAST.PROC")
	field(LNK2, "$(P)READ_SLOW1.PROC")
	field(LNK3, "$(P)READ_FAST.PROC")
	field(LNK4, "$(P)READ_SLOW2.PROC")
	field(LNK5, "$(P)READ_FAST.PROC")
	field(LNK6, "$(P)READ_SLOW3.PROC")
	field(LNK7, "$(P)READ_FAST.PROC")
	field(LNK8, "$(P)READ_SLOW4.PROC")
	
	field(SELM, "All")
}

record(seq, "$(P)READ_RESTART"){
	field(SCAN, "1 second")
	field(LNK1, "$(P)READ.PROC")
}

record(seq, "$(P)READ_FAST") {
	field(SCAN, "Passive")
	field(DO1, "1")
	field(DO2, "1")
	field(DLY1, "$(TDLY)")
	field(DLY2, "$(TDLY)")
	field(SELM, "All")
	field(LNK1, "$(P)RBV.PROC")
	field(LNK2, "$(P)OUTPUT.PROC")
}

