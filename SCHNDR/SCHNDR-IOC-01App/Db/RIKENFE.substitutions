                                                                               "LV3CLOSE",         "NOT Close LV3", "-",         "!Close LV3",
    }

    { "LV4:ILK:STAT", "lv_interlock_status",  "3", "LV4 Interlock Status",        "COMMS",        "Communications", "-",        "Comms Error",
                                                                                   "FSOV",           "FSOV Opened", "-",        "FSOV Opened",
                                                                                   "GH30",                  "GH30", "-",            "GH30 OK",
                                                                           "BIT3CONSTANT",            "Constant 0", "-",                  "0",
                                                                                   "GH24",                  "GH24", "-",            "GH24 OK",
