# 2.4.2    Vacuum Pump LV Interlocks (LV - Line Valve)
# Note: Bits 2 & 3 and 4 & 5 are ORed together and then ANDed with bits 1,6,7 and 8 to obtain the valve interlock.

file RIKENFE_LV_Interlock_Status.template {
    pattern {PV, PORTNAME, OFFSET, DESC, BIT0PV, BIT0DESC, BIT0ZNAM, BIT0ONAM,
                                         BIT1PV, BIT1DESC, BIT1ZNAM, BIT1ONAM,
                                         BIT2PV, BIT2DESC, BIT2ZNAM, BIT2ONAM,
                                         BIT3PV, BIT3DESC, BIT3ZNAM, BIT3ONAM,
                                         BIT4PV, BIT4DESC, BIT4ZNAM, BIT4ONAM,
                                         BIT5PV, BIT5DESC, BIT5ZNAM, BIT5ONAM,
                                         BIT6PV, BIT6DESC, BIT6ZNAM, BIT6ONAM,
                                         BIT7PV, BIT7DESC, BIT7ZNAM, BIT7ONAM,
                                         BIT8PV, BIT8DESC, BIT8ZNAM, BIT8ONAM,
    }

    { "LV1:ILK:STAT", "lv_interlock_status",  "0", "LV1 Interlock Status",        "COMMS",        "Communications", "-",        "Comms Error", 
                                                                                   "FSOV",           "FSOV Opened", "-",        "FSOV Opened", 
                                                                                   "GH16",                  "GH16", "-",            "GH16 OK",
                                                                                   "GH12",                  "GH12", "-",            "GH12 OK",
                                                                                    "GH8",                   "GH8", "-",             "GH8 OK",
                                                                           "BIT5CONSTANT",            "Constant 0", "-",                  "0",
                                                                               "BB0CLOSE", "NOT BB0 Close Command", "-", "!BB0 Close Command", 
                                                                           "BIT7CONSTANT",            "Constant 1", "-",                  "1",
                                                                               "LV1CLOSE",         "NOT Close LV1", "-",         "!Close LV1",
    }

    { "LV2:ILK:STAT", "lv_interlock_status",  "1", "LV2 Interlock Status",        "COMMS",        "Communications", "-",        "Comms Error",
                                                                                   "FSOV",           "FSOV Opened", "-",        "FSOV Opened",
                                                                                   "GH20",                  "GH20", "-",            "GH20 OK",
                                                                                   "GH24",                  "GH24", "-",            "GH24 OK",
                                                                                   "GH12",                  "GH12", "-",            "GH12 OK",
                                                                                   "GH16",                  "GH16", "-",            "GH16 OK",
                                                                           "BIT6CONSTANT",            "Constant 1", "-",                  "1",
                                                                           "BIT7CONSTANT",            "Constant 1", "-",                  "1",
                                                                               "LV2CLOSE",         "NOT Close LV2", "-",         "!Close LV2",
    }

    { "LV3:ILK:STAT", "lv_interlock_status",  "2", "LV3 Interlock Status",        "COMMS",        "Communications", "-",        "Comms Error",
