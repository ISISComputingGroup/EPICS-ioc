# 2.4.3    Vacuum Pump Other Valve Interlocks

file RIKENFE_Other_Valve_Interlock_Status.template {
    pattern {PV, PORTNAME, OFFSET, DESC, BIT0PV, BIT0DESC, BIT0ZNAM, BIT0ONAM,
                                         BIT1PV, BIT1DESC, BIT1ZNAM, BIT1ONAM,
                                         BIT2PV, BIT2DESC, BIT2ZNAM, BIT2ONAM,
                                         BIT3PV, BIT3DESC, BIT3ZNAM, BIT3ONAM,
                                         BIT4PV, BIT4DESC, BIT4ZNAM, BIT4ONAM,
                                         BIT5PV, BIT5DESC, BIT5ZNAM, BIT5ONAM,
                                         BIT6PV, BIT6DESC, BIT6ZNAM, BIT6ONAM,
                                         BIT7PV, BIT7DESC, BIT7ZNAM, BIT7ONAM,
                                         BIT8PV, BIT8DESC, BIT8ZNAM, BIT8ONAM,
    }

    { "AMGV:ILK:STAT", "other_valve_interlock_status",  "0", "AMGV Interlock Status",        "COMMS",        "Communications", "-",        "Comms Error", 
                                                                                              "FSOV",           "FSOV Opened", "-",        "FSOV Opened", 
                                                                                               "GH8",                   "GH8", "-",             "GH8 OK",
                                                                                      "BIT3CONSTANT",            "Constant 0", "-",                  "0",
                                                                                               "GH4",                   "GH4", "-",             "GH4 OK",
                                                                                      "BIT5CONSTANT",            "Constant 0", "-",                  "0",
                                                                                      "BIT6CONSTANT",            "Constant 1", "-",                  "1",
                                                                                      "BIT7CONSTANT",            "Constant 1", "-",                  "1",
                                                                                      "BIT8CONSTANT",            "Constant 1", "-",                  "1",
    }

    { "FSOV:ILK:STAT", "other_valve_interlock_status",  "1", "FSOV Interlock Status",        "COMMS",        "Communications", "-",        "Comms Error", 
                                                                                      "BIT1CONSTANT",            "Constant 1", "-",                  "1",    
