record(ai, "$(P)DDS_STRESS_RIG:ELONG") {
    field(DESC, "Elongation read from array")
    field(INP, "$(P)DDS_STRESS_RIG:ARRAY.[0] CP MS")
}

record(ai, "$(P)DDS_STRESS_RIG:LOAD") {
    field(DESC, "Load read from array")
    field(INP, "$(P)DDS_STRESS_RIG:ARRAY.[1] CP MS")
}

record(ao, "$(P)DDS_STRESS_RIG:LOAD:SP") {
    field(DESC, "Set-point for load")
}

record(ai, "$(P)DDS_STRESS_RIG:TIME") {
    field(DESC, "Time read from Array")
    field(INP, "$(P)DDS_STRESS_RIG:ARRAY.[2] CP MS")
}

record(calc, "$(P)DDS_STRESS_RIG:MOT:ACTIV") {
    field(DESC, "description")
    field(CALC, "a!=0")
    field(INPA, "$(P)DDS_STRESS_RIG:MOT:STAT CP MS")
}

record(seq, "$(P)DDS_STRESS_RIG:CHANGE_SPEED"){
    field(DESC, "Comnand Sequence to set Speed.")
    field(DO0, "1")
    field(LNK0, "$(P)DDS_STRESS_RIG:MODE:SPEED:SP PP")
    field(DO1, "0")
    field(LNK1, "$(P)DDS_STRESS_RIG:CONTROL:SPEED:SP PP")
    field(DO2, "0")
    field(LNK2, "$(P)DDS_STRESS_RIG:DIRECTION:SPEED:SP PP")
    field(DOL3, "$(P)DDS_STRESS_RIG:SPEED:SP")
    field(LNK3, "$(P)DDS_STRESS_RIG:_SPEED:SP PP")
    field(DO4, "1")
    field(LNK4, "$(P)DDS_STRESS_RIG:SPEED:SET:SP PP")
    field(DO5, "2")
    field(LNK5, "$(P)DDS_STRESS_RIG:DIRECTION:SPEED:SP PP")
    field(DLY5, "0.2")
    field(DO6, "1")
    field(LNK6, "$(P)DDS_STRESS_RIG:SPEED:SET:SP PP")
    field(DLY6, "0.2")
    field(SCAN, "Passive")
    field(SELM, "All")
}

record(ao, "$(P)DDS_STRESS_RIG:SPEED:SP") {
    field(DESC, "Target Value")
}

record(seq, "$(P)DDS_STRESS_RIG:START"){
    field(DESC, "Comnand Sequence to request a Start.")
    field(DO0, "1")
    field(LNK0, "$(P)DDS_STRESS_RIG:MEAS:STOP:SP PP")
    field(DO1, "1")
    field(LNK1, "$(P)DDS_STRESS_RIG:CHANNEL:SP PP")
    field(DO2, "0")
    field(LNK2, "$(P)DDS_STRESS_RIG:ULIMIT:ENABLED:SP PP")
    field(DO3, "0")
    field(LNK3, "$(P)DDS_STRESS_RIG:LLIMIT:ENABLED:SP PP")
    field(DO4, "2")
    field(DLY4, "0.1")
    field(LNK4, "$(P)DDS_STRESS_RIG:CHANNEL:SP PP")
    field(DO5, "0")
    field(DLY5, "0.1")
    field(LNK5, "$(P)DDS_STRESS_RIG:CON:STAT:SP PP")
    field(DO6, "0")
    field(DLY6, "0.1")
    field(LNK6, "$(P)DDS_STRESS_RIG:MOT:MODE:SP PP")
    field(DO7, "1")
    field(DLY7, "0.2")
    field(LNK7, "$(P)DDS_STRESS_RIG:MOT:MODE:SP PP")
    field(DOL8, "$(P)DDS_STRESS_RIG:TRGT")
    field(DLY8, "0.2")
    field(LNK8, "$(P)DDS_STRESS_RIG:UPPER_LIMIT:SP PP")
    field(DOL9, "$(P)DDS_STRESS_RIG:TRGT")
    field(DLY9, "0.2")
    field(LNK9, "$(P)DDS_STRESS_RIG:LOWER_LIMIT:SP PP")
    field(DOA, "1")
    field(DLYA, "0.2")
    field(LNKA, "$(P)DDS_STRESS_RIG:ULIMIT:ENABLED:SP PP")
    field(DOB, "1")
    field(DLYB, "0.2")
    field(LNKB, "$(P)DDS_STRESS_RIG:LLIMIT:ENABLED:SP PP")
    field(DOC, "1")
    field(DLYC, "0.2")
    field(LNKC, "$(P)DDS_STRESS_RIG:MEAS:STOP:SP PP")
    field(DOD, "1")
    field(DLYD, "0.2")
    field(LNKD, "$(P)DDS_STRESS_RIG:MEAS:START:SP PP")
    field(SCAN, "Passive")
    field(SELM, "2")
    field(SELL, "$(P)DDS_STRESS_RIG:_START_LIMIT PP")
    field(SHFT, "0")
}

record(ao, "$(P)DDS_STRESS_RIG:TRGT") {
    field(DESC, "Target Value")
}

record(calc, "$(P)DDS_STRESS_RIG:_START_LIMIT") {
    field(DESC, "description")
    field(CALC, "a>b?13823:15103")
    field(INPA, "$(P)DDS_STRESS_RIG:TRGT CP MS")
    field(INPB, "$(P)DDS_STRESS_RIG:ELONG CP MS")
}
