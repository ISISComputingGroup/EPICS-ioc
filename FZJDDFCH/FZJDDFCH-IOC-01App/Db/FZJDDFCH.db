record(ai, "$(P)MBON")
{
    field(DESC, "Status of magnetic bearings ON|OFF")
    field(SCAN, "1 second")
    field(DTYP, "stream")
    field(INP,  "@devFZJ_DDFermiChopper.proto getMBON(A) $(PORT)")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:MBON")
    field(SDIS, "$(P)DISABLE")
    info(INTEREST, "MEDIUM")
    info(archive, "VAL")
}
