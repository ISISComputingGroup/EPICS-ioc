
record(ai, "$(P)SLEW1")
{
    field(DESC, "Slew DAC 1")
    field(DTYP, "stream")
    field(INP,  "@mps8000-232.proto rslew(1) $(port) 0")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:SLEW1")
    field(SDIS, "$(P)DISABLE")
}

record(ao, "$(P)SLEW1:SP")
{
    field(DESC, "Set Slew DAC 1")
    field(DTYP, "stream")
    field(OUT,  "@mps8000-232.proto wslew(1) $(port) 0")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:SLEW1:SP")
    field(SDIS, "$(P)DISABLE")
}

