record(bi, "$(P)ILK")
{
    field(INP, "$(P)STA1.B6 CP MS")
    field(DESC, "Overall Interlock")
    field(ZNAM, "OK")
    field(ONAM, "Interlock")
    info(INTEREST, "HIGH")
    info(archive, "VAL")
}

record(calcout, "$(P)POWER:_CALC")
{
    field(INPA, "$(P)STA1.BF CP MS")
	field(CALC, "!A")
}

record(bi, "$(P)POWER")
{
    field(INP, "$(P)POWER:_CALC CP MS")
    field(DESC, "Power")
    field(ZNAM, "Off")
    field(ONAM, "On")
    info(INTEREST, "HIGH")
    info(archive, "VAL")
}
alias("$(P)POWER", "$(P)POWER:RBV")

record(ao, "$(P)RAW:SP")
{
    field(DESC, "Set Output Current")
    field(EGU,  "mA")
    field(DTYP, "stream")
    field(OUT,  "@dfkps.proto dac(0) $(port) 0")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:RAW:SP")
    field(SDIS, "$(P)DISABLE")
    field(PINI, "$(SP_PINI)")
    info(autosaveFields, "VAL")
}

record(ao, "$(P)SIM:RAW"){
field(DTYP, "Soft Channel")
field(EGU, "A")
}
alias("$(P)SIM:RAW", "$(P)SIM:RAW:SP")

record(ai, "$(P)RAW")
{
field(DESC, "ADC Channel 8")
field(DTYP, "stream")
field(INP, "@dfkps_common.proto adc(8) $(port) 0")
field(SCAN, "1 second")
field(LOLO, "-999999.5")
field(HIHI, "99999.5")
field(LLSV, "MAJOR")
field(HHSV, "MAJOR")
field(HOPR, "100000")
field(LOPR, "0")
field(PREC, "0")
field(FLNK, "$(P)CURR")
field(SIML, "$(P)SIM")
field(SIOL, "$(P)SIM:RAW")
field(SDIS, "$(P)DISABLE")
field(EGU, "A")
info(INTEREST, "HIGH")
info(archive, "VAL")
}

