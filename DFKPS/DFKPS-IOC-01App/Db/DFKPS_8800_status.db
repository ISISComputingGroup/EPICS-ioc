record(ao, "$(P)RAW:SP")
{
    field(DESC, "Set Output Current")
    field(EGU,  "mA")
    field(DTYP, "stream")
    field(OUT,  "@dfkps.proto dac $(port) 0")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:RAW:SP")
    field(SDIS, "$(P)DISABLE")
    field(EGU, "")
    field(PINI, "$(SP_PINI)")
    info(autosaveFields, "VAL")
}
