record(seq, "$(PSU_DISABLE):$(PSNAME)")
{
    field(DOL1, "$(PSU_DISABLE) CP")
	field(LNK1, "$(PV_PREFIX)RKNPS_0$(PSGRP):$(PSNAME):POWER:SP.DISP PP")
}


record(calcout, "$(P)PSUS:POWER:$(PSNAME)")
{
    field(INPA, "$(PV_PREFIX)RKNPS_0$(PSGRP):$(PSNAME):POWER CP MS")
	field(CALC, "A")
	field(OUT, "$(P)PSUS:POWER:GRP$(PSGRP).$(PSIDX) PP MS")
	field(OOPT, "Every Time")
}

