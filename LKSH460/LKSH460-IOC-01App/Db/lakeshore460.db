record(stringin, "$(P)$(R)IDN")
{
    field(DESC, "SCPI identification string")
    field(DTYP, "stream")
    field(INP,  "@devLakeshore_460.proto getIDN $(PORT)")
    field(PINI, "YES")
  #  field(SIML, "$(P)SIM")
   # field(SIOL, "$(P)SIM:IDN")
    field(SDIS, "$(P)DISABLE")
}

